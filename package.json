{
  "name": "eazy-pomodoro-chrome-extension",
  "version": "1.0.0",
  "description": "A Pomodoro Chrome Extension in React and TypeScript using a simple Webpack build process",
  "scripts": {
    "start": "webpack --watch --progress --config webpack.dev.js",
    "build": "webpack --progress --config webpack.prod.js",
    "compile:popup": "node-sass src/sass/main-popup.scss src/popup/popup.css -w"
  },
  "author": "Otuokwu Chibuzor",
  "devDependencies": {
    "@types/chrome": "^0.0.181",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.2.4",
    "html-webpack-plugin": "^4.5.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.16.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.8",
    "framer-motion": "^10.16.4",
    "node-sass": "^9.0.0",
    "react-chrome-extension-router": "^1.4.0",
    "react-hook-form": "^7.47.0"
  }
}
